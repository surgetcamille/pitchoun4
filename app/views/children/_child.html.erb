<div id="<%= dom_id child %>">
  <p>
    <strong>Picture:</strong>
    <%= child.picture %>
  </p>

  <p>
    <strong>Name:</strong>
    <%= child.name %>
  </p>

  <p>
    <strong>Firstname:</strong>
    <%= child.firstname %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= child.age %>
  </p>

  <p>
    <strong>Country:</strong>
    <%= child.country %>
  </p>

  <p>
    <strong>City:</strong>
    <%= child.city %>
  </p>

</div>
