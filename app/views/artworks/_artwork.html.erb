<div id="<%= dom_id artwork %>">
  <p>
    <strong>Paint:</strong>
    <%= artwork.paint %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= artwork.title %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= artwork.date %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= artwork.author %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= artwork.description %>
  </p>

</div>
